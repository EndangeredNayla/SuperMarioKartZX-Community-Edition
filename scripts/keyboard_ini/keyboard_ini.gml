function keyboard_ini() {
	for (var j=0;j<4;j++)
	{
		for (var i=0;i<key_numb;i++)
		{
			joykey[j,i]=noone
			//show_message(joykey[j,i])
		}
	}
	joykey[0,0]=vk_right
	joykey[0,1]=vk_left
	joykey[0,2]=vk_up
	joykey[0,3]=vk_down
	joykey[0,4]=ord("X")
	joykey[0,5]=ord("Z")
	joykey[0,6]=noone//ord("S")
	joykey[0,7]=ord("C")
	joykey[0,8]=vk_enter
	joykey[0,9]=vk_space
	global.multicon=0


}
